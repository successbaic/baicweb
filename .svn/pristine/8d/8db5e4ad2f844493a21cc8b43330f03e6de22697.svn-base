@charset "utf-8";
/*style 1*/
.eIDUI_select div {
	font-family: "Microsoft YaHei", "WenQuanYi Micro Hei Mono","WenQuanYi Micro Hei", "WenQuanYi Zen Hei", "Hiragino Sans GB","Hiragino Sans GB W3", "黑体", "宋体", Arial;
}

.eIDUI_select {
	width: 210px;
	padding-right: 30px;
	display: inline-block;
	*display: inline;
	*zoom: 1;
	height: 58px;
	font-size: 12px;
	line-height: 30px;
	position: relative;
	z-index: 9999;
	vertical-align: middle;
	background: url(./images/ddicon2.png) no-repeat right top;
}

.eIDUI_select:hover {
	/*background: url(ddicon.png) no-repeat right -30px;*/
	
}

.eIDUI_nodevice {
	background: url(./images/nodevice.gif) no-repeat center;
    background: url(./images/nodevice.gif) no-repeat center 0px\9;
	width: 250px;
	height: 180px;
	display:inline-block;
	background-size:contain;
    /*filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='./images/nodevice.gif',sizingMethod='scale');*/
}

.eIDUI_select .eIDUI_selectckd {
	display: block;
	height: 53px;
	margin-top: 0px;
	padding-left: 2px;
	padding-top: 3px;
	cursor: default;
	border-left: 1px #d6dae4 solid;
	border-bottom: 1px #d6dae4 solid;
	border-top: 1px #d6dae4 solid;
	font-size: 14px;
	line-height: 25px;
	/*font-family: 'Microsoft YaHei', '黑体', '宋体';*/
}

.eIDUI_select .eIDUI_selectlist {
	display: none;
	position: absolute;
	top: 58px;
	left: 0;
	width: 100%;
	_width: 100px;
}

.eIDUI_select ol {
	border: 1px solid #d6dae4;
	list-style: none;
	margin: 0px;
	padding: 0px;
}

.eIDUI_select ol li {
	border-bottom: 1px solid #d6dae4;
	padding: 0 0 0 10px;
	background: #fff;
	cursor: pointer;
	height: 55px;
	line-height: 25px;
	font-size: 14px;
	list-style: none;
	margin: 0px;
	padding: 0px;
	/*font-family: 'Microsoft YaHei', '黑体', '宋体';*/
}

.eIDUI_select ol li div {
	padding-left: 0px;
}

.eIDUI_select ol li:hover {
	cursor: pointer;
	background: #fafafa;
	color: #0081cb;
}

.eIDUI_dtl3 {
	/*width: 240px;*/
	display:inline-block;
}

.eIDUI_btndiv2 {
	float: clear;
	text-align: center;
}

.eIDUI_plugin_setup { /*font-family: 'Microsoft YaHei', '黑体', '宋体';*/
	font-size: 14px;
	color: white;
	width: 212px;
	height: 54px;
	background: url(./images/btnnoplugin.png) no-repeat;
	cursor: pointer;
	/*left: 50px;*/
	position: relative;
	border: none;
}

.eIDUI_btndivt {
	margin-top: 20px;
	float: clear;
	background: url(./images/icon-1.png) no-repeat 20px 0px;
	font-size: 12px;
	text-align: center;
	font-family: "Microsoft YaHei", "WenQuanYi Micro Hei Mono","WenQuanYi Micro Hei", "WenQuanYi Zen Hei", "Hiragino Sans GB","Hiragino Sans GB W3", "黑体", "宋体", Arial;
}