	public boolean doAdd() throws SQLException
	{
		DbConnection dbc=new DbConnection();
		ResultSet rsCount;
		int nRowCount;
		String sqlAdd="INSERT INTO GGGL_GGJY(PRIPID,ANCHEYEAR,ENTREGNO,ENTNAME,ENTTYPE,ENTCAT,ENTPRO,ENTCLASS,MEDCLASS,BUSTUR,RATAL,MEDI,FOOD,HTHFOOD,MAQUI,BEACUL,MEDAPP,HTHCARPRO,TOBACCO,WINE,ADMREC,REAEST,AGRMAT,SERVICE,INFORIND,FININS,AUTOMAN,CONEQU,CLOTH,ELSBUSTUR,EMPNUM,MANNUM,WORNUM,ELSEMPNUM,MANAGER,FILLMAN,FILLDAT,APPMAN,APPUNIT,APPDATE,STATUS,APPOPI,ISPRESS,PRETION,ISOUTAD,OUTAD,FILLTEL,LINKMAN,QUAGRADE,POSTCODE,OPRAINCOME,OUTOPRAINCOME,SHOWCARD,LIGHTBOX,ELECSIGNS,NENOLIGHT,VEHICLE,OTHERNUM,ENTADRESS,REGCAPITAL,LEGREFEREE,MEDINUM,DJJG) VALUES("+ doPreProcess3(PRIPID)+","+ANCHEYEAR+","+ doPreProcess3(ENTREGNO)+","+ doPreProcess3(ENTNAME)+","+ doPreProcess3(ENTTYPE)+","+ doPreProcess3(ENTCAT)+","+ doPreProcess3(ENTPRO)+","+ doPreProcess3(ENTCLASS)+","+ doPreProcess3(MEDCLASS)+","+BUSTUR+","+RATAL+","+MEDI+","+FOOD+","+HTHFOOD+","+MAQUI+","+BEACUL+","+MEDAPP+","+HTHCARPRO+","+TOBACCO+","+WINE+","+ADMREC+","+REAEST+","+AGRMAT+","+SERVICE+","+INFORIND+","+FININS+","+AUTOMAN+","+CONEQU+","+CLOTH+","+ELSBUSTUR+","+EMPNUM+","+MANNUM+","+WORNUM+","+ELSEMPNUM+","+ doPreProcess3(MANAGER)+","+ doPreProcess3(FILLMAN)+",to_date("+doPreProcess3(FILLDAT)+",'yyyy-mm-dd'),"+ doPreProcess3(APPMAN)+","+ doPreProcess3(APPUNIT)+",to_date("+doPreProcess3(APPDATE)+",'yyyy-mm-dd'),"+ doPreProcess3(STATUS)+","+ doPreProcess3(APPOPI)+","+ doPreProcess3(ISPRESS)+","+PRETION+","+ doPreProcess3(ISOUTAD)+","+OUTAD+","+ doPreProcess3(FILLTEL)+","+ doPreProcess3(LINKMAN)+","+ doPreProcess3(QUAGRADE)+","+ doPreProcess3(POSTCODE)+","+OPRAINCOME+","+OUTOPRAINCOME+","+SHOWCARD+","+LIGHTBOX+","+ELECSIGNS+","+NENOLIGHT+","+VEHICLE+","+OTHERNUM+","+ doPreProcess3(ENTADRESS)+","+REGCAPITAL+","+ doPreProcess3(LEGREFEREE)+","+MEDINUM+","+ doPreProcess3(DJJG)+")";
		{							
			nRowCount=dbc.executeUpdate( sqlAdd );
			dbc.close();
			if (nRowCount>0)	//插入成功
				return true;
			else
				return false;
		}
	}

	public boolean doCount() throws SQLException
	{
		DbConnection dbc=new DbConnection();
		ResultSet rsCount;
		int nRowCount;
		String sqlCount="SELECT COUNT(*) FROM GGGL_GGJY"+" WHERE ANCHEYEAR="+ANCHEYEAR+" and PRIPID='"+doPreProcess(PRIPID)+"'";
		rsCount=dbc.executeQuery( sqlCount );
		rsCount.next();
		nRowCount=rsCount.getInt( 1 );

		dbc.close();
		if (nRowCount>0)
		{
			return true;
		}
		else
			return false;
	}

